{
   "background": {
      "scripts": [ "/libs/jquery.js", "/js/screenshot.js", "/js/background.js" ]
   },
   "browser_action": {
      "default_icon": "images/128.png",
      "default_popup": "/popup.html",
      "default_title": "Screenshoter"
   },
   "content_scripts": [ {
      "js": [ "/libs/jquery.js", "/libs/detect_zoom.js", "/libs/cropper.js", "/js/plugin.js", "/js/content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "images/128.png",
      "32": "images/32.png",
      "64": "images/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_name__",
   "offline_enabled": true,
   "permissions": [ "activeTab", "pageCapture", "downloads", "tabs", "storage", "webRequest", "webRequestBlocking", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.3",
   "web_accessible_resources": [ "/images/*.png", "/libs/jquery.js" ]
}

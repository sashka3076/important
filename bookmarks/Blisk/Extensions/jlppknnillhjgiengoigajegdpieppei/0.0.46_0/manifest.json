{
   "background": {
      "scripts": [ "global.js", "global-chrome.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "IconDisabled.png",
         "38": "IconDisabled@2x.png"
      },
      "default_title": "Enable LiveReload"
   },
   "content_scripts": [ {
      "js": [ "injected.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "RemoteLiveReload",
   "permissions": [ "tabs", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.46",
   "web_accessible_resources": [ "livereload.js" ]
}

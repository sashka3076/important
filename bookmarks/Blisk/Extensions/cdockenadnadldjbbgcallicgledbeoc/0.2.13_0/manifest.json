{
   "background": {
      "persistent": true,
      "scripts": [ "visbug.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/visbug.png"
      },
      "default_title": "VisBug"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Alt+Shift+D",
            "linux": "Alt+Shift+D",
            "mac": "Alt+Shift+D",
            "windows": "Alt+Shift+D"
         }
      }
   },
   "description": "Open source browser design tools",
   "icons": {
      "128": "icons/visbug.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "VisBug",
   "permissions": [ "activeTab", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.13",
   "web_accessible_resources": [ "tuts/*" ]
}
